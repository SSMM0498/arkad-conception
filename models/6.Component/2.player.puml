@startuml "player component diagram"
skinparam shadowing false
skinparam monochrome true

frame "cp: Player" {
    [Player] -up- [PlayerDOM]
    [PlayerDOM] -up- [NodeBuilder]
    [Player] -left- [ActionScheduler]
    [ActionScheduler] -left- [ActionTriggers]
    [Player] -right- [PlayerMachineState]
    [Player] -down- [PlayerElement]
}
@enduml