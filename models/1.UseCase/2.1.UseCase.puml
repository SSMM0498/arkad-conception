@startuml UseCase
left to right direction
skinparam shadowing false
skinparam monochrome true

frame "uc: Creator - Authentification" {
    actor Creator

    rectangle System {
        (Log in)
        (Recover password)
        (With Google)
        (With Github)
    }

    :Creator: --> (Log in)
    (Log in) ..> (Recover password) : << extend >>
    (Log in) <|-- (With Google)
    (Log in) <|-- (With Github)
}

@enduml